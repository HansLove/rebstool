{
  "entry": [
    "src/main.tsx",
    "src/app/App.tsx"
  ],
  "project": [
    "src/**/*.{ts,tsx,js,jsx}"
  ],
  "ignore": [
    "**/*.d.ts",
    "**/*.config.{js,ts}",
    "**/node_modules/**",
